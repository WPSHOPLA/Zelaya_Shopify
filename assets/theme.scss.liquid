.sizeswatch .swatch{margin: 0em 1em 1em 1em;}
.sizeswatch .swatch label:before{content:"[";}
.sizeswatch .swatch label:after{content:"]";}
.sizeswatch .swatch input:checked+label{width:45px}
.sizeswatch .swatch label {
  /* Rounded corners */
 
  border-radius:0px;
  /* To give width and height */
  float:left;
  /* Color swatches contain no text so they need to have a width. */
  min-width:{{ width }} !important; 
