  }
}

.cart-items-container {
  display: block;
  padding: 0 80px 0 0;

  @include breakpoint(md) {
    width: 520px;
    padding: 0;
    margin: auto;
  }

  @include breakpoint(sm) {
    width: 100%;
  }

  .order & {
    padding: 0;
  }
}

// TABLE

.cart-items {
  width: 100%;
  margin-top: 0;
  border-collapse: collapse;
  border: 0;
  @include transition(height 0.2s $ease);

  &.empty {
    height: 0;
    overflow: hidden;
  }

  thead {
    @include breakpoint(md) {
      display: none;
    }
  }

  th {
    padding: 20px 0;
    font-weight: 300;
    font-family: $body-font;
    color: $heading-text-color;
    text-align: left;
    border-top: none;
    border-bottom: 1px solid $border-color;

    &.first {
      padding-left: 0;
    }

    &.last {
      padding-right: 0;
      text-align: right;
    }
  }

  tr:nth-child(odd) {
    @include breakpoint(md) {
      background-color: $very-light-grey;
    }
  }

  tr:nth-child(even) {
    @include breakpoint(md) {
      background-color: $light-grey;
    }
  }

  .mobile-only {
    display: none;

    @include breakpoint(md) {
      display: inline-block;
    }
  }
}

.cart-item {
  position: relative;
  @include transition(opacity 0.3s ease-in, height 0.3s ease-in);

  &.removing {
    opacity: 0;
  }
}

.cart-item-td {
  width: 15%;
  padding: 40px 0;
  line-height: 1;
  text-align: left;
  color: $body-text-color;
  vertical-align: middle;
  border: 0;
  border-bottom: 1px solid $border-color;

  @include breakpoint(md) {
    float: left;
    width: 520px;
    padding: $gutter/2 $gutter/2 0;
    margin: 0;
    border: 0;
    color: $body-text-color;
    text-align: center;

    &:first-child {
      padding-top: $gutter;
    }

    &:last-child {
      padding-bottom: $gutter/2;
    }
  }

  @include breakpoint(sm) {
    width: 100%;
  }
}

.cart-item-quantity-wrapper {
  @include breakpoint(md) {
    width: 100%;
    margin: auto;
  }

  @include breakpoint(sm) {
    width: 100%;
  }
}

.cart-title {
  a {
    font-size: 14px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: $secondary-color;
  }
}

.cart-item-unit-price {
  @include breakpoint(md) {
    text-align: center;
  }
}

.cart-item-product {
  width: 55%;

  @include breakpoint(md) {
    width: 100%;
    text-align: center;
  }
}

.cart-item-product-wrap {
  display: inline-block;
  margin-left: $gutter/2;
  vertical-align: middle;

  @include breakpoint(l) {
    max-width: 250px;
  }

  @include breakpoint(md) {
    width: 100%;
    max-width: 100%;
    margin: 0;
  }

  span {
    display: block;
    padding: 5px 0 0;
  }
}

.cart-item-image-container {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  width: 80px;

  @include breakpoint(md) {
    display: block;
    width: 100%;
    padding-bottom: $gutter/2;
    text-align: center;
  }
}

.cart-item-total-container {
  position: relative;
  display: block;
  text-align: right;

  @include breakpoint(md) {
    text-align: center;
    padding-bottom: $gutter/2;
  }
}

.cart-image {
  display: inline-block;
  width: 100%;
  vertical-align: middle;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }

  @include breakpoint(sm) {
    display: block;
    width: 100px;
    margin: 0 auto $gutter/2;
  }
}

.cart-item-remove {
  position: absolute;
  left: 100%;
  top: 50%;
  width: 40px;
  height: 40px;
  margin: -20px 0 0 30px;
  vertical-align: middle;
  color: $primary-color;
  text-align: center;
  cursor: pointer;

  @include breakpoint(md) {
    left: auto;
    top: -25px;
    right: -10px;
    margin: 0;
    display: none;
  }

  .icon {
    width: 100%;
    height: 100%;
    line-height: 40px;
    font-size: 28px;
  }

  &.mobile-only {
    display: none;

    @include breakpoint(md) {
      display: block;
    }
  }

  &:hover {
    color: $primary-color;
  }
}

// TOTALS

.cart-tools {
  position: relative;
  display: block;
  text-align: right;
}

.cart-totals {
  @extend .clearfix;
  display: block;
  margin: 0;
  padding: 0 80px 0 0;
  text-align: right;

  @include breakpoint(md) {
    padding: 0;
  }
}

.cart-price-info {
  color: $heading-text-color;

  @include breakpoint(md) {
    text-align: center;
  }
}

.sub-total,
.cart-price {
  padding: 0;
}

.cart-message,
.cart-currency-note {
  display: block;
  font-size: 13px;
  line-height: 23px;
  font-style: italic;
  color: $body-text-color;
  text-align: right;
  padding: 0;

  @include breakpoint(md) {
    display: block;
    margin: auto;
    max-width: 320px;
    text-align: center;
  }
}

.cart-price {
  font-size: $heading-font-size-2;
  line-height: $heading-font-size-2 + 10;
  color: $heading-text-color;
}

// CTA

.cart-cta {
  margin-top: $gutter;
  @include breakpoint(md) {
    max-width: 320px;
    margin-right: auto;
    margin-left: auto;
    text-align: center;
  }

  input { vertical-align: middle; }
}

.cart-button {
  min-width: $button-min-width;

  &:last-child { margin-right: 0; }

  @include breakpoint(md) {
    padding-right: 0;
    padding-left: 0;
    margin-top: $gutter / 2;
    text-align: center;
  }

  @include breakpoint(md-up) {
    &.cart-update-button {
      margin-right: $gutter / 2;
    }
  }
}

.cart-additional-checkout-buttons {
  display: inline-block;
  vertical-align: middle;

  @include breakpoint(md-up) {
    margin-right: - $gutter / 4;
    margin-left: $gutter / 2;
  }

  input {
    margin-top: 0;
    margin-right: $gutter / 4;
    margin-left: $gutter / 4;

    @include breakpoint(md) {
      margin-top: $gutter / 2;
    }
  }

  //scss-lint:disable ImportantRule
  .shopify-apple-pay-button {
    margin-bottom: 0 !important;

    @include breakpoint(md-up) {
      margin-top: 0 !important;
      margin-right: $gutter / 4 !important;
      margin-left: $gutter / 4 !important;
    }

    @include breakpoint(md) {
      margin-top: $gutter / 2 !important;
    }
  }
}

// INSTRUCTIONS

.cart-instructions {
  display: block;
  font-size: $body-font-size-1;
  padding: $gutter*2 80px 0 0;
  text-align: left;

  @include breakpoint(md) {
    width: 290px;
    padding: $gutter 0 0;
    margin: auto;
  }

  p {
    text-align: left;
    padding: 0 0 $gutter/2;
  }

  textarea {
    font-size: 14px;
  }
}

.cart-checkout {
  margin-top: $gutter;
}

.cart-modal-wrapper {
  @extend %modal-wrapper;

  .cart-modal { @extend %modal; }
  .cart-modal .section-title { @extend %modal-title; }
  .cart-modal-action { @extend %modal-action-button; }
}

.cart-item-undo {
  @include breakpoint(sm) {
    text-align: center;
  }
}

.cart-undo {
  cursor: pointer;
  color: $primary-color;

  &:hover {
    color: $primary-color;
  }
}

.cart-shipping-calculator-wrapper {
  font-size: 0;
  margin-top: $gutter*2;

  @include breakpoint(sm) {
    padding-right: $gutter;
    padding-left: $gutter;
  }
}

.cart-shipping-calculator {
  width: 60%;
  display: inline-block;
  vertical-align: top;
  font-size: $body-font-size-1;
  text-align: left;

  @include breakpoint(sm) {
    width: 100%;
    text-align: center;
  }
}

.cart-shipping-calculator-response {
  width: 40%;
  display: inline-block;
  vertical-align: top;
  font-size: 14px;
  padding: 39px 0 0 $gutter;
  text-align: right;

  @include breakpoint(sm) {
    width: 100%;
    text-align: center;
    padding: 0;
  }
}

.shipping-country-wrapper, .shipping-province-wrapper {
  margin-top: 15px;

  .select-wrapper {
    margin: 0;
  }
}

.cart-shipping-zip-code input {
  display: inline-block;
  width: auto;
  font-size: 14px;
  min-width: 270px;

  @include breakpoint(sm) {
    width: 100%;
    margin: 0;
  }

  &[type='text'] {
    margin-right: $gutter/2;
    margin-bottom: $gutter/2;
  }
}

input[type="button"].cart-shipping-calculate {
  @include breakpoint(sm) {
    margin: 15px 0 0 0;
  }
}

// DEFAULT

.collection {
  width: 100%;
  padding: $gutter*3 0;

  @include breakpoint(sm) {
    padding: $gutter 0 0;
  }

  .style-light &,
  .style-color &,
  .style-gradient & {
    margin-top: $gutter*2;
  }
}

.collection-header {
  padding-bottom: $gutter;
}

.collection-tag-selector {
  display: inline-block;
  margin: $gutter auto 0;
  vertical-align: top;

  .select-wrapper {
    width: 200px;
    background-color: $white;
  }
}

.collection-button {
  @include breakpoint(sm) {
    width: 290px;
    padding-right: 0;
    padding-left: 0;
    text-align: center;
  }
}

.collection-list {
  width: 100%;
  margin-top: $gutter;
}

.collection-subtitle {
  padding: $gutter $gutter 0;

  @include breakpoint(sm) {
    padding: $gutter $gutter/2 0;
  }
}

// ALTERNATING

.collection-alternating {
  max-width: 100%;
  padding-bottom: 0;
}

.collection-alternating-product {
  position: relative;
  overflow: hidden;
  font-size: 0;

  &:nth-child(odd) {
    background-color: rgba($black,0.02);
  }

  &:first-child {
    margin-top: 0;
    padding-top: 0;
    border-top: none;
  }
}

.collection-alternating-section {
  display: inline-block;
  width: 50%;
  vertical-align: middle;

  @include breakpoint(md) {
    position: relative;
    width: 100%;
    display: block;
    overflow: hidden;
  }

  &.right {
    text-align: right;
  }

  &.left {
    text-align: left;
  }

  &.right,
  &.left {
    text-align: center;
  }
}

.collection-alternating-media {
  position: relative;
  display: none;
  padding: 0;
  overflow: hidden;

  &.show {
    display: inline-block;

    @include breakpoint(md) {
      display: none;
    }
  }

  &.show-on-mobile {
    @include breakpoint(md) {
      display: block;
    }
  }
}

.collection-alternating-cover {
  @include dummy(80%);
  display: none;
  background-color: $black;

  .crop & {
    display: block;
  }
}

.collection-alternating-image {
  .crop & {
    display: none;
  }
}

.collection-alternating-text-wrapper {
  padding: $gutter*3 $gutter*2;
  text-align: left;

  @include breakpoint(l) {
    padding: $gutter;
  }

  @include breakpoint(md) {
    display: inline-block;
    max-width: 700px;
    padding: $gutter*3 $gutter;
    text-align: center;
  }

  @include breakpoint(sm) {
    max-width: 100%;
    padding: $gutter;
  }
}

.collection-alternating-title {
  font-size: $heading-font-size-2;
  line-height: $heading-font-size-2 + 10;

  @include breakpoint(sm) {
    font-size: $heading-font-size-2-xs;
    line-height: $heading-font-size-2-xs + 10;
  }
}

.collection-alternating-meta {
  font-size: $heading-font-size-4;
  line-height: $heading-font-size-4 + 10;
  text-transform: uppercase;
}

.collection-alternating-text {
  font-size: $body-font-size-1;
  line-height: $body-font-size-1 + 10;
  padding-top: $gutter;

  @include breakpoint(md) {
    margin: $gutter auto 0;
    padding: 0;
  }
}

.collection-alternating-cta {
  padding-top: $gutter;

  @include breakpoint(md) {
    padding-top: $gutter;
  }
}

// MISC

.original.money {
  text-decoration: line-through;
}

.list-meta .saving {
  display: block;
}

.sale-badge {
  @include border-radius(100%);
  position: absolute;
  top: 20px;
  right: 20px;
  width: $sale-badge-size-large;
  height: $sale-badge-size-large;
  font-family: $heading-font;
  font-size: $body-font-size-2;
  font-weight: 300;
  line-height: $sale-badge-size-large;
  letter-spacing: 0.1em;
  color: $white;
  text-align: center;
  text-transform: uppercase;
  background: $secondary-color;

  @include media($max: $ipad){
    width: $sale-badge-size-small;
    height: $sale-badge-size-small;
    font-size: $body-font-size-3;
    line-height: $sale-badge-size-small;
  }

  .collection-alternating-section.right & {
    right: 50%;
    margin-right: 20px;

    @include breakpoint(md) {
      right: 0;
    }
  }
}

.stock-badge {
  @include transform(rotate(-45deg));
  position: absolute;
  top: $stock-badge-size * 2;
  left: -32px;
  width: 150px;
  padding: $stock-badge-size / 2 0;
  font-family: $heading-font;
  font-size: $stock-badge-size;
  font-weight: 300;
  line-height: 1;
  letter-spacing: 0.1em;
  color: $white;
  text-align: center;
  text-transform: uppercase;
  background: $secondary-color;

  .collection-alternating-section.left & {
    left: 50%;

    @include breakpoint(md) {
      left: 0;
    }
  }
}
.contact .contact-form-title{font-size:15px;}
.contact-form {
  position: relative;
  width: 100%;

  .input-wrapper {
    margin: $gutter/2 0;

    &.first {
      margin-top: 0;
    }

    &.last {
      margin-bottom: 0;
      text-align:center;
    }
  }
}

.contact-form-button {
  margin-top: $gutter;
    width: 160px;
    font-size: 14px;
    padding: 12px 15px;

  @include breakpoint(sm) {
   margin-top: $gutter/2;
  }
}
.contact-form-bgc {background-color:#f0f0f0;	}
.contact-form-success {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba($white,0.6);
}

.contact-form-success-icon {
  @include border-radius(100%);
  position: absolute;
  top: 50%;
  left: 50%;
  height: 160px;
  width: 160px;
  margin-left: -80px;
  margin-top: -80px;
  line-height: 160px;
  text-align: center;
  font-size: 42px;
  color: $white;
  background-color: rgba($primary-color,0.8);
}

.contact-form-error-message {
  display: block;
  margin-bottom: $gutter;
  color: $error-color;
}

.faq {
  @include breakpoint(sm) {
    padding: $gutter $gutter/2;
  }
}

.faq-module.style-default {

  .module {
    padding-top: 0;
  }
}

.faq-content {

  .module {
    padding-bottom: 0;

    &.no-content {
      padding-bottom: $gutter * 2;

      @include breakpoint(sm) { padding-bottom: $gutter; }
    }
  }

  .module-body {
    padding-bottom: $gutter * 2;
  }
}


.template-index {
  .main-content {
    width: 100%;
    max-width: 100%;
    padding: 0;
  }

  .sticky-header.full-window-slideshow ~ .main-content,
  .sticky-header.lower-than-slideshow ~ .main-content {
    padding-top: 0;
  }
}

.collections {
  padding: $gutter*3 0;

  @include breakpoint(md) {
    padding: 0;
  }
}

.collections-header {
  padding-bottom: $gutter*3;

  @include breakpoint(md) {
    padding: $gutter*3 $gutter;
  }

  @include breakpoint(sm) {
    padding: $gutter $gutter/2;
  }
}

.page-header {
  padding-bottom: $gutter;
  margin: 0 $gutter $gutter;

  @include breakpoint(sm) {
    padding-bottom: $gutter/2;
    margin: $gutter $gutter/2;
  }

  &:only-child {
    padding-bottom: 0;
    margin-bottom: 0;
  }
}

.page-body {
  max-width: 700px;
  width: 100%;
  text-align: left;
  margin: auto;

  @include breakpoint(sm) {
    max-width: 100%;
    padding: 0 $gutter/2;
  }

  p:first-child {
    padding-top: 0;
  }

  p:last-child {
    padding-bottom: 0;
  }
}

.template-product {

  .main-content {
    font-size: 0;
  }

  .page-title {
    @include breakpoint(sm) {
      margin-bottom: $gutter;
    }
  }
}

.template-product-slideshow {

  .main-content {
    width: 100%;
    max-width: 100%;
    padding: 0;
  }

  @include breakpoint(sm) {
    .home-slideshow-wrapper + div {
      padding: 0 30px;
    }
  }

  .page-title {
    margin: 60px 0 45px;

    @include breakpoint(sm) {
      margin: $gutter 0;
    }
  }
}

.product-slideshow, .product-wrap {
  display: inline-block;
  vertical-align: top;
  width: 50%;
  font-size: $body-font-size-1;

  @include breakpoint(sm) {
    display: block;
    width: 100%;
    padding: 0;
  }
}
.sharetitle{color:#282828;font-family: "Open Sans",sans-serif;font-size: 22px;text-align:center; font-weight: bold;}
.module-product {
  padding: $gutter*2 0 $gutter*3;

  @include breakpoint(l) {
    padding: $gutter*2 $gutter $gutter*3;
  }

  @include breakpoint(sm) {
    padding: $gutter 0;
  }
}

// PRODUCT: SLIDESHOW

.product-slideshow {
  position: relative;
  padding-right: $gutter * 2 / 3;

  @include breakpoint(sm) {
    max-width: 600px;
    padding-right: 0;
    margin-top: $gutter;
    margin-right: auto;
    margin-left: auto;
  }
}

.product-image-container {
  position: relative;
  display: block;
}

.product-big-image {
  position: relative;
  display: block;
  padding: 0;

  @include breakpoint(md) {
    padding-right: 0;
  }

  img {
    display: block;
    margin: 0 auto;
    max-width: 75%;
  }

  &.working {

    img {
      visibility: hidden;
    }
  }

  &.zoom-enabled {
    cursor: pointer;
    cursor: -webkit-zoom-in;
    cursor: -moz-zoom-in;
    cursor: zoom-in;
  }
}

.product-slideshow-loader {
  @extend .add-to-cart-loader;
  left: 50%;
  right: auto;
  z-index: 1;
  display: none;

  .working & {
    display: block;
  }
}

.product-image-zoom {
  position: absolute;
  background-color: white;
  background-repeat: no-repeat;
  display: none;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 10;

  cursor: pointer;
  cursor: -webkit-zoom-out;
  cursor: -moz-zoom-out;
  cursor: zoom-out;

  &.active {
    display: block;
  }
}

.product-thumbnails {
  margin-top: $gutter;
  text-align: center;
  font-size: 0;

  &.hidden {
    visibility: hidden;
  }
}

.product-no-images {
  width: 100%;
  padding: 35%;
  background: #fafafa;

  .no-svg & {
    padding: 0;
    background: none;
  }
}

.product-slideshow-pagination-item {
  @include transition(all 0.2s $ease);
  @include background-size(cover);
  display: inline-block;
  vertical-align: top;
  width: 75px;
  height: 75px;
  padding: 2px;
  margin: 0 10px 10px 0;
  background-position: center;
  background-repeat: no-repeat;
  opacity: 0.5;

  &.active {
    opacity: 1;
  }
}

.product-slideshow-pagination-item-image {
  display: none;
}

.product-slideshow-navigation {
  @include transition(all $slideshowSpeed $ease);
  position: absolute;
  top: 50%;
  width: $gutter*2;
  height: 60px;
  line-height: 60px;
  margin-top: -30px;
  z-index: 2;
  text-align: center;
  font-size: 28px;
  cursor: pointer;
  color: $primary-color;
}

.product-slideshow-previous {
  left: 0;
}

.product-slideshow-next {
  right: 0;
}

// PRODUCT: INFO WRAP

.product-wrap {
  text-align: left;
  padding-left: $gutter;

  @include breakpoint(sm) {
    padding: 0 $gutter/2;

    .product-goal {
      padding-right: 20px;
      padding-left: 20px;
    }
  }
}

// PRODUCT DETAILS

.product-details {

  @include breakpoint(sm) {
    display: none;
  }

  &.mobile {
    display: none;
    text-align: center;

    @include breakpoint(sm) {
      display: block;
      padding-right: $gutter / 2;
      padding-left: $gutter / 2;
    }
  }
}

.product-title {
  display: block;
  margin: 0 0 5px;
  color: $heading-text-color;

  h1 {
    font-size: 34px;
    line-height: 44px;
    padding: 0;
    margin: 0;
  }
}

.product-vendor {
  color: $body-text-color;
}

.product-price {
  display: block;
  font-size: 28px;
  line-height: 38px;
  color: #b99800;
}

.product-price-compare {
  display: inline-block;
  margin-left: 10px;
  color: $body-text-color;
  font-size: 14px;

  .original {
    text-decoration: line-through;
  }

  span {
    display: inline-block;
    vertical-align: top;
  }
}

// ADD TO CART BUTTON

.add-to-cart-button {
  @include transition(all 0.2s $ease);
  position: relative;
  opacity: 1;

  .loading & {
    z-index: -1;
    opacity: 0;
  }
}

// PRODUCT FORM

.product-form {
  display: block;
  margin: $gutter 0 0;
}

.product-form-nav {
  display: block;
}

.product-options,
.product-quantity {
  visibility: hidden;

  &.visible {
    visibility: visible;
  }

  &.hidden {
    display: none;
  }

  .no-js & {
    visibility: visible;
  }

  @include breakpoint(sm) {
    width: 100%;
  }
}

.product-options {

  .selector-wrapper {
    float: left;
    width: 45%;
    margin: 0 $gutter/2 $gutter/2 0;

    @include breakpoint(md) {
      width: 100%;
      margin: 0 0 $gutter/2;
    }
  }

  .selected-text {
    @include ellipsis();
    display: block;
    width: 90%;
  }

  label {
    display: block;
  }
}

.add-to-cart {
  margin: $gutter/2 0 0;

  @include breakpoint(sm) {
    text-align: center;
  }

  input {
    @include breakpoint(sm) {
      padding-right: 0;
      padding-left: 0;
      width: 290px;
    }
  }
}

.product-form-submit-wrap {
  display: inline-block;
  position: relative;

  @include breakpoint(sm) {
    text-align: center;
  }

  input {
    position: relative;
    z-index: 2;
    white-space: normal;
  }
}

@include keyframes(rotation) {
  from { @include transform(rotate(0deg)) }
  to { @include transform(rotate(359deg)) }
}

.add-to-cart-loader {
  position: absolute;
  left: 50%;
  top: 50%;
  height: 30px;
  width: 30px;
  margin-left: -15px;
  margin-top: -15px;
  z-index: 1;
  border-left: 3px solid rgba($primary-color, .15);
  border-right: 3px solid rgba($primary-color, .15);
  border-bottom: 3px solid rgba($primary-color, .15);
  border-top: 3px solid rgba($primary-color, .8);
  @include border-radius(100%);
  @include animation(rotation .6s infinite linear);
  @include transition(right 0.2s $ease);
  display:none;
  .loading & {
    right: -45px;
  }
}

.product-add-success-message, .product-add-error-message {
  display: none;
  color: $body-text-color;
  margin-top: $gutter;
}

.added-success .product-add-success-message {
  display: block;
}

.added-error .product-add-error-message {
  display: block;
  color: $error-color;
}

.added-product-name {
  font-style: italic;
  color: $body-text-color;
}

.product-more-info {
  margin: $gutter 0 0;

  @include breakpoint(sm) {
    text-align: center;
  }
}

.product-info-link {
  position: relative;
  display: block;
  margin: 5px 0 0;
  padding: 0;

  &:first-child {
    margin: 0 auto;
  }

  &:before {
    position: relative;
    display: inline-block;
    @include share-icons();
    color: $primary-color;
  }

  &:hover:before {
    color: $primary-color;
  }

  &.info:before {
    content: $icon-info;
  }

  &.question-mark:before {
    content: $icon-questionmark;
  }

  &.ruler:before {
    content: $icon-ruler;
  }

  &.envelope:before {
    content: $icon-email;
  }

  &.pencil:before {
    content: $icon-pencil;
  }

  &.shirt:before {
    content: $icon-shirt;
  }
}

.search {
  margin-top: $gutter*2;

  @include breakpoint(sm) {
   margin: 0;
  }
}

.search-header {
  padding: 0 0 $gutter*2;

  @include breakpoint(sm) {
    padding: $gutter $gutter/2;
  }
}

.search-form {
  position: relative;
  text-align: center;
  font-size: 0;
  max-width: 450px;
  margin: $gutter auto 0;

  @include breakpoint(sm) {
    margin: $gutter/2 auto 0;
  }
}

.search-input {
  display: block;
  width: 100%;
  font-size: $body-font-size-1;
}

.search-submit {
  position: absolute;
  display: block;
  right: 0;
  top: 0;
  width: 50px;
  height: 50px;
  background: $primary-color;
  @include border-radius(0 4px 4px 0);

  &:hover {
    background: $primary-color;
  }

  input[type="submit"] {
    position: absolute;
    left: 0;
    right: 0;
    width: 50px;
    height: 50px;
    padding: 0;
    font-size: 0;
    background: transparent;
    border: transparent;
    @include border-radius(0);

    &:hover {
      background: transparent;
      border: transparent;
    }
  }
}

.search-submit-icon {
  position: absolute;
  left: 0;
  right: 0;
  width: 50px;
  height: 50px;
  line-height: 50px;
  font-size: 24px;
  color: $white;
}

.search-results-wrapper {
  margin: $gutter auto 0;
}

.search-results-count {
  text-align: center;
  margin: $gutter 0 $gutter*2;

  ul {
    list-style: none;
    padding: 0;
  }
}

$account-module-max-width: 600px;

.module-login,
.module-register,
.module-account,
.module-reset-password,
.module-addresses,
.module-activate-account {
  .module-title {
    @include breakpoint(sm) {
      padding-top: $gutter;
    }
  }

  .module-subtitle {
    max-width: $account-module-max-width;
    margin: 0 auto;
  }

  .module-content {
    @include border-radius(5px);
    max-width: $account-module-max-width;
    margin: $gutter auto 0;
    padding: $gutter;
    background-color: $white;
    border: 1px solid $border-color;
    text-align: left;

    @include breakpoint(sm) {
      @include border-radius(0);
    }
  }
}

.account-info {
  margin-bottom: $gutter;

  > span {
    display: block;
  }
}

.view-addresses-link {
  font-size: 14px;
}

.orders {
  width: 100%;
  margin: $gutter 0;

  td {
    text-align: center;
  }
}

.decline-activation {
  margin-left: 10px;

  @include breakpoint(sm) {
    display: block;
    margin: 10px 0 0 0;
  }
}

.customer-address-name {
  font-weight: 700;
}

.cta-container {
  @include breakpoint(sm) {
    text-align: center;
  }

  .button {
    margin-right: $gutter / 2;

    @include breakpoint(sm) {
      margin-right: $gutter / 4;
      margin-left: $gutter / 4;
      margin-bottom: $gutter / 2;

      .module-register & { margin-right: $gutter / 2; }

      &:last-child { margin-bottom: 0; }
    }

    @include breakpoint(xs) {
      margin-right: 0;
      margin-left: 0;
    }
  }
}

.template-customers-activate {

}
.customer-address-edit-form, .customer-new-address {
  display: none;

  &.show {
    display: block;
  }

  label ~ .select-wrapper {
    margin-top: $gutter/2;
  }
}

.customer-address-edit-form {
  .input-wrapper:first-of-type {
    margin-top: 0;
  }
}

.customer-address.editing {
  display: none;
}

.customer-address-wrap {
  display: inline-block;
  width: 100%;
  margin-top: $gutter;
  padding-top: $gutter;
  vertical-align: top;
  border-top: 1px solid $border-color;

  @include breakpoint(sm) {
    width: 100%;
    margin: $gutter 0;
    padding: 0;
  }

  &:first-child {
    margin: 0;
    padding: 0;
    border-top: 0;

    @include breakpoint(sm) {
      margin-top: 0;
    }
  }
}

.customer-address {
  position: relative;
  font-size: $body-font-size-1;

  p:first-child {
    margin-top: 0;
  }

  p:last-child {
    margin-bottom: 0;
  }
}

.edit-address, .delete-address {
  color: $primary-color;
  cursor: pointer;
  font-size: 14px;

  &:hover {
    color: $primary-color;
  }
}

.add-new-address-wrapper {
  padding-top: $gutter*2;
  margin-top: $gutter;
  border-top: 1px solid $border-color;

  &.no-border {
    padding-top: 0;
    border-top: none;
  }

  .input-wrapper:first-of-type {
    margin-top: 0;
  }
}

.default-address-marker {
  @include border-radius(5px);
  display: block;
  padding: 10px $gutter / 2;
  margin-bottom: $gutter / 2;
  font-size: $body-font-size-2;
  letter-spacing: 0.1em;
  color: $white;
  text-transform: uppercase;
  background-color: $secondary-color;
}

.customer-address-wrap .pagination {
  padding: 0;
}

.recover-password {
  display: none;

  .reset-button {
    display: inline-block;
    margin: 0;
  }
}

.successful-reset {
  display: none;
}

.toggle-forgetfulness {
  display: inline-block;
  vertical-align: top;
  line-height: $button-height;
  padding: 0 $gutter/2;
  cursor: pointer;

  @include breakpoint(sm) {
    padding: 0;
    display: block;
  }

  &:hover {
    color: $primary-color;
  }
}

.guest-checkout, .new-customer {
  margin-top: $gutter*2;
}

.module-register .already-customer {
  display: inline-block;
  padding: $gutter/2 0;
}

.template-customers-order {

  .cart-item-quantity {
    font-size: $body-font-size-1;
  }
}

.order-header {
  margin-bottom: $gutter*3;

  @include breakpoint(sm) {
    margin-bottom: $gutter;
  }
}

.order-totals {
  text-align: right;
  width: 33%;
  float: right;
  margin-top: $gutter;

  @include breakpoint(sm) {
    margin: $gutter auto 0;
    float: none;
  }

  td {
    padding: 2px 0 2px 10px;
    text-align: right;

    @include breakpoint(sm) {
      width: 50%;
    }
  }

  td:first-child {
    padding: 2px 10px 2px 0;
  }

  td:last-child {
    @include breakpoint(sm) {
      text-align: left;
    }
  }
}

.order-total {
  color: $heading-text-color;
  font-weight: 700;
}

.shipping-info {
  font-size: 0;
  width: 66%;
  margin-top: $gutter*2;

  @include breakpoint(sm) {
    width: 100%;
  }

  > div {
    display: inline-block;
    width: 50%;
    vertical-align: top;
    font-size: $body-font-size-1;

    @include breakpoint(sm) {
      text-align: center;
      width: 100%;

      &:last-child {
        margin-top: $gutter;
      }
    }
  }

  h2 {
    font-size: 24px;
    margin-top: 0;
  }
}

.order-history .mobile-layout tr td {
  border-top-width: 0 !important;

  &:first-child {
    border-top-width: 1px !important;
  }
}

@media screen and (max-width: 360px) {
  .feature-img-bg-2{padding: 0 0;}
}