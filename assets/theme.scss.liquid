.rte {
  .tabs {
    position: relative;
    z-index: 10;
    display: block;
    width: 100%;
    height: 30px;
    padding: 0;
    margin: 30px 0 0;
    line-height: 30px;
    white-space: nowrap;
    border-bottom: 1px solid $border-color;

    &:first-child { margin-top: 0; }

    li {
      position: relative;
      display: inline-block;
      padding: 0;
      margin: 0 0 0 10px;
      font-size: 13px;
      letter-spacing: 2px;
      text-transform: uppercase;
      cursor: pointer;
      opacity: 0.8;

      &:first-child { margin-left: 0; }

      @include breakpoint(sm) {
        font-size: 12px;
      }

      a { color: inherit; }

      &.active {
        font-weight: 700;
        color: $heading-text-color;
        cursor: default;

        &:after {
          position: absolute;
          bottom: 1px;
          left: 0;
          width: 100%;
          height: 1px;
          background: $primary-color;
          content: "";
        }
      }
    }
  }

  .tabs-content {
    position: relative;
    top: -1px;
    z-index: 5;
    display: block;
    width: 100%;
    padding: $gutter / 2 0 0;
    margin: 0;
    overflow: hidden;

    > li {
      display: none;
      margin: 0;

      &.active { display: block; }

      ul,
      ol {
        padding-left: 20px;
      }

      > *:first-child {
        margin-top: 0;
      }

      > *:last-child {
        margin-bottom: 0;
      }
    }
  }
}

