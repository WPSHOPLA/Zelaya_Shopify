%modal-wrapper {
  display: none;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000;
  background-color: rgba($white,0.6);

  &.active {
    display: block;
  }
}

%modal {
  @include transform(translate(-50%,-50%));
  @include box-shadow();
  position: absolute;
  top: 50%;
  left: 50%;
  max-width: 640px;
  padding: $gutter*3;
  border: 1px solid $border-color;
  text-align: center;
  background-color: $body-background-color;
}

%modal-action-button {
  margin-top: $gutter;
}

%modal-title {
  margin-bottom: 18px;
}

.module-list-item-content-toggle {
  max-width: 700px;
  margin: auto;

  @include breakpoint(md) {
    max-width: 100%;
  }

  @include breakpoint(sm) {
    padding: $gutter $gutter/2;
  }
}

.module-list-item-content-toggle-item-header {
  position: relative;
  display: block;
  padding: 0 0 $gutter;
  cursor: pointer;
}

.module-list-item-content-toggle-item-title {
  padding-left: 40px;
  text-align: left;
  font-size: $heading-font-size-3;
  line-height: $heading-font-size-3 + 10;

  @include breakpoint(sm) {
    font-size: $heading-font-size-3-xs;
    line-height: $heading-font-size-3-xs + 10;
  }

  .style-default &,
  .style-light & {
    color: $heading-text-color;
  }
}

.module-list-item-content-toggle-item-header-icon {
  position: absolute;
  top: 0;
  left: 0;
  width: $gutter;
  height: $heading-font-size-2;
  line-height: $heading-font-size-2;
  text-align: center;
  font-size: 21px;

  &.icon-angle-up {
    display: none;
  }

  .active & {
    &.icon-angle-down {
      display: none;
    }

    &.icon-angle-up {
      display: block;
    }
  }

  .style-default &,
  .style-light & {
    color: $primary-color;
  }
}

.module-list-item-content-toggle-item-body {
  display: none;
  padding: 0 0 $gutter 40px;
  text-align: left;

  .active & {
    display: block;
  }
}

.header-tools .navigation {
  position: relative;
  display: inline-block;
  height: $header-height;
  line-height: $header-height;
  vertical-align: top;
  text-align: left;

  @include breakpoint(sm) {
    display: none;
  }

  .collapsed-navigation & {
    display: none;
}

  .has-dropdown.open .secondary-list,
  .has-dropdown:active .secondary-list,
  .has-dropdown:hover .secondary-list {
    top: 100%;
    left: -80px;
  }
  
  .has-dropdown:active .secondary-list {
    display: block;
  }
}

.header-navigation {
  position: relative;
  z-index: 1000;
  background: $body-text-color;

  &.sticky-header {
    border-bottom: 0;
    top: 120px;

    .touch & {
      top: auto;
    }
  }

  @include breakpoint(sm) {
    display: none;
  }
}

// LINK LISTS

.header-navigation-list {
  display: inline-block;
  vertical-align: middle;
  margin: 0;
  padding: 0;

  .header-drawer & {
    padding: 75px 0;

    @include breakpoint(sm) {
      padding: $gutter*2 0;
    }
  }
}

.header-navigation-list-item {
  position: relative;
  float: left;
  padding: 20px 0;
  margin: 0;

  .header-drawer & {
    width: 100%;
    padding: $gutter/2 $gutter/2 0;

    &:first-child {
      padding: 0;
    }
  }
}

.primary-link {
  height: 20px;
  padding: 0;
  margin: 0 15px;

  .slide-color-dark &:after { background-color: $black; }
  .slide-color-light &:after  { background-color: $white; }

  .slide-color-light &,
  .slide-color-dark & {
    position: relative;

    &:focus:after,
    &:hover:after {
      position: absolute;
      bottom: -49px;
      left: 0;
      z-index: -1;
      display: block;
      width: 100%;
      height: 1px;
      content: "";
    }
  }
}

.header-navigation-link {
  @include font-smoothing();
  float: left;
  font-size: $header-font-size;
  font-weight: normal;
  line-height: 20px;
  letter-spacing: 1px;
  color: $body-text-color;

  &:focus,
  &:hover {
    color: $primary-color;
  }

  @media (min-width: 721px) {
    &:not(.secondary-link) {
      .has-slideshow-full-window.slide-color-dark .header:not(.scrolled) .main-header:not(.collapsed-navigation) & { color: $black; }
      .has-slideshow-full-window.slide-color-light .header:not(.scrolled) .main-header:not(.collapsed-navigation) & { color: $white; }
    }
  }

  .slide-color-light &,
  .slide-color-dark & {
    position: relative;

    &:focus,
    &:hover {
      opacity: 1;
    }

    &.header-navigation-current:after {
      display: none;
    }
  }

  .slide-color-light .scrolled &,
  .slide-color-dark .scrolled & {
    color: $body-text-color;
    opacity: 1;
  }

  &.header-navigation-current {
    color: #b99800;
    position: relative;
    font-weight: normal;

    &:focus,
    &:hover {
      color: $primary-color;
    }

    &:after {
      display:none;

      .header-drawer & {
        display: none;
      }
    }

    .header-bleed-light-colors & {
      color: $white;
      font-weight: 700;
      opacity: 1;

      &:focus,
      &:hover {
        color: $white;
      }

      &:after {
        background: $white;
      }
    }

    .header-bleed-dark-colors & {
      color: $black;
      font-weight: 700;
      opacity: 1;

      &:focus,
      &:hover {
        color: $black;
      }

      &:after {
        background: $black;
      }
    }

    .showing-drawer & {
      color: $white;
      font-weight: inherit;

      &:focus,
      &:hover {
        color: $white;
      }
    }
  }
  
  .header-drawer & {
    display: inline-block;
    float: none;
    height: auto;
    margin: 0;
    text-align: center;
    vertical-align: top;
    font-family: $heading-font;
    font-size: $drawer-font-size;
    line-height: $drawer-font-size + 10;
    color: $white;

    &:hover {
      color: $primary-color;
    }
    
    &:after {
      display: none !important;
    }
  }
}

.secondary-list {
  @include border-radius(5px);
  display: block;
  position: absolute;
  top: -9999px;
  left: -9999px;
  width: 160px;
  margin: 0 0 0 50%;
  padding: $gutter/2 0;
  text-align: center;
  background-color: $white;
  border: 1px solid $border-color;

  &:before, &:after {
    bottom: 100%;
    left: 50%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(136, 183, 213, 0);
  }

  &:before {
    border-bottom-color: $border-color;
    border-width: 8px;
    margin-left: -8px;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        border-bottom-color: $white;
      }

      .header-bleed-dark-colors & {
        border-bottom-color: $black;
      }
    }
  }

  &:after {
    border-bottom-color: $white;
    border-width: 7px;
    margin-left: -7px;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        border-bottom-color: $white;
      }

      .header-bleed-dark-colors & {
        border-bottom-color: $black;
      }
    }
  }

  .header-drawer & {
    @include border-radius(0);
    position: relative;
    display: none;
    top: auto;
    left: auto;
    float: left;
    width: 100%;
    margin: 0;
    padding: $gutter/2 0 0;
    background-color: transparent;
    border: 0;

    &:before, &:after {
      display: none;
    }
  }

  .open & {
    display: block;
  }
}

