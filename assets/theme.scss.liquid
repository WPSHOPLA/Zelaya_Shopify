$slideshowSpeed: 0.7s;

.home-slideshow-wrapper {
  @include transition(height $slideshowSpeed $ease);
  overflow: hidden;
  min-height: 250px;
  position: relative;

  .has-slideshow-full-window & {
    height: 100vh;
  };

  // Legacy selector to limit the slide's width to the content
  &.content-width {
    max-width: $max-width - $gutter*2;
    padding: 0 $gutter;
    margin: $gutter*2 auto;
  }

  &.full-width {
    width: 100%;
  }

  * {
    -webkit-tap-highlight-color: rgba(0,0,0,0);
  }
}

.home-slideshow-loader {
  @extend .add-to-cart-loader;
  left: 50%;
  right: auto;
  z-index: 1;
  display:block;

  .slides-ready & {
    display: none;
  }
}

.home-slideshow {
  @include transition(opacity $slideshowSpeed $ease);
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  opacity: 0;

  .slides-ready & {
    opacity: 1;
  }
}

.home-slideshow-navigation {
  @include transition(all $slideshowSpeed $ease);
  display: block;
  width: 70px;
  height: 100%;
  position: absolute;
  z-index: 5;
  top: 0;
  text-align: center;
  font-size: 32px;
  cursor: pointer;
  visibility: hidden;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;

  .slide-color-light & { color: $white; }
  .slide-color-dark & { color: $black; }

  @include breakpoint(sm) {
    width: 30px;
  }

  .slides-ready & {
    visibility: visible;
  }
}

.home-slideshow-previous {
  left: 0;
}

.home-slideshow-next {
  right: 0;
}

.home-slideshow-pagination {
  @include transform(translateX(-50%));
  @include transition(color $slideshowSpeed $ease);
  position: absolute;
  bottom: 0;
  z-index: 5;
  text-align: center;
  left: 50%;
  line-height: 50px;
  font-size: 0;
  visibility: hidden;

  .slide-color-light & { color: $white; }
  .slide-color-dark & { color: $black; }

  .slides-ready & {
    visibility: visible;
  }

  .pagination-list-item {
    width: 12px;
    height: 12px;
    margin: 0 3px;
    display: inline-block;
    cursor: pointer;
  }

  .pagination-list-item span {
    @include border-radius(100%);
    width: 100%;
    height: 100%;
    display: block;
    background: transparent;
    border: 1px solid;

    .slide-color-light & { border-color: $white; }
    .slide-color-dark & { border-color: $black; }

    &:hover {
      border-width: 2px;
    }
  }

  .active span {
    background-color: $body-text-color;

    .slide-color-light & { background-color: $white; }
    .slide-color-dark & { background-color: $black; }
  }
}

.home-slide {
  @include transition(height $slideshowSpeed $ease);
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: $body-background-color;

  .full-window & {
    height: 100%;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;

    @include breakpoint(sm) {
      background: none !important; // override inline styles
    }
  }
}

.transition-fade .home-slide {
  opacity: 0;

  &.active {
    z-index: 1;
    opacity: 1;
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(opacity $slideshowSpeed $ease);
  }

  &.next.left,
  &.prev.right {
    opacity: 1;
  }

  &.active.left {
    opacity: 0;
  }
  &.active.right {
    opacity: 0;
  }
}

.transition-slide .home-slide {
  z-index: 1;
  visibility: hidden;

  &.active {
    @include transform(translateX(0));
    z-index: 2;
    visibility: visible;
  }

  &.next {
    @include transform(translateX(100%));
  }

  &.prev {
    @include transform(translateX(-100%));
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(transform $slideshowSpeed $ease);
  }

  &.next.left,
  &.prev.right {
    @include transform(translateX(0));
    visibility: visible;
    z-index: 10;
  }

  &.active.left {
    @include transform(translateX(-100%));
  }
  &.active.right {
    @include transform(translateX(100%));
  }
}

.slide-image-container {
  @include background-size(cover);
  display: block;
  width: 100%;
  height: 100%;
  background-color: transparent;
  background-position: center;
  background-repeat: no-repeat;
}

.slide-image {
  @include no-select();
  display: block;
  width: 100%;
  opacity: 0;

  @include breakpoint(sm) {
    height: 100%;
    max-width: none;
  }

  .full-window & {
    display: none;
  }
}

.slide-text {
  @include transform(translate(-50%,-50%));
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  max-width: 1200px;
  z-index: 1;

  @include breakpoint(md) {
    max-width: 700px;
  }

  .full-window & { padding-top: 60px; }

  .text-aligned-center & {
    padding: $gutter*2 $gutter*2 0;
    text-align: center;

    @include breakpoint(sm) {
      padding: 0 $gutter;
    }
  }

  .text-aligned-left & {
    padding-right: 30%;
    padding-left: $gutter;

    @include lt-ie9 {
      padding-left: 0;
      left: 100px;
    }

    @media (max-width: 1370px) {
      @include transform(translate(0,-50%));
      padding-left: 0;
      left: 100px;
    }

    @include breakpoint(sm) {
      @include transform(0);
      left: 0;
      padding: 0 $gutter;
      text-align: center;
    }

    .content-width & {
      @include transform(translate(0,-50%));
      left: 100px;
      padding-left: 0;
    }
  }

  .text-aligned-right & {
    padding-left: 30%;
    padding-right: $gutter;
    text-align: right;

    @include lt-ie9 {
      right: 100px;
      left: auto;
      padding-right: 0;
    }

    @media (max-width: 1370px) {
      @include transform(translate(0,-50%));
      padding-right: 0;
      right: 100px;
      left: auto;
    }

    @include breakpoint(sm) {
      @include transform(0);
      left: 0;
      padding: 0 $gutter;
      text-align: center;
    }

    .content-width & {
      @include transform(translate(0,-50%));
      padding-right: 0;
      right: 100px;
      left: auto;
    }
  }

  &.light {

    h2,
    h3,
    p {
      color: $white;
    }
  }

  &.dark {
    h2,
    h3,
    p {
      color: $black;
    }
  }

  h2,
  h3,
  p {

    &:first-child {
      margin-top: 0;
    }

    @include breakpoint(sm) {
      margin: 0;
    }
  }

  h2,
  h3,
  p,
  .button {
    &:last-child {
      margin-bottom: 0;
    }
  }

  p {
    opacity: 0.9;
  }
  .button span.icon-angle-right{font-size:19px}
  .button {
    min-width: $button-min-width;
    margin: ($gutter * 3) 0 0;
    visibility: visible;

    @include breakpoint(md) {
      margin: $gutter 0 0;
    }
  }
}

.slide-text h2 {
  font-family: $heading-font;
  font-weight: $heading-weight;
  font-size: $heading-font-size-1-xs;
  line-height: $heading-font-size-1 + 10;
  text-transform:uppercase;
  letter-spacing:4px;
  @include breakpoint(md) {
    font-size: $heading-font-size-2;
    line-height: $heading-font-size-2 + 10;
  }

  @include breakpoint(sm) {
    font-size: $heading-font-size-2-xs;
    line-height: $heading-font-size-2-xs + 10;
  }
}

.slide-text h3 {
  padding-top: $gutter/2;
  font-family: $heading-font;
  font-weight: $heading-weight;
  font-size: $heading-font-size-5;
  line-height: $heading-font-size-5 + 10;
   text-transform:uppercase;
  letter-spacing:2px;

  @include breakpoint(md) {
    padding-top: 0;
  }
}

