.main-footer-module {
  float: left;
}

.footer-title {
  display: block;
  padding: 0;
  margin: 0;
  font-size: 12px;
  font-weight: 700;
  line-height: 1.1;
  color: $heading-text-color;
  text-transform: uppercase;

  .style-dark & { color: $white; }
}

// LOGO

.footer-site-logo-container {
  display: block;
  width: 20%;
  padding: 0 0 $gutter;
  text-align: left;

  &.wide-logo-container {
    width: 50%;
    padding: 0;
  }

  &:only-child {
    width: 100%;
  }

  &,
  &.wide-logo-container {

    @include breakpoint(md) {
      width: 50%;
    }

    @include breakpoint(xs) {
      width: 100%;
    }
  }
}

.footer-site-logo-anchor {
  display: inline-block;
  width: 100%;
  padding-right: 20px;
  vertical-align: top;

  @if ($logo-use-image) {
    max-width: $logo-max-width;
  }
}

.footer-logo-alternate,
.footer-logo-regular {
  display: block;
  width: $logo-width;
}

.footer-logo-image {
  width: auto;
  height: auto;
  max-width: $logo-max-width;
  max-height: $logo-max-height;
  padding-right: $gutter;
  box-sizing: border-box;
}

.branding-logo {
  margin: 0;
  font-family: $heading-font;
  font-weight: $heading-weight;
  color: $primary-color;
  font-size: 28px;
  line-height: inherit;
}

// LINK-LISTS

.footer-link-list-title {
  letter-spacing: 1px;
}

.footer-link-list-container {
  float: left;
  width: 50%;

  &.container-of-1:nth-child(-n + 2) {
    @include breakpoint(md-up) { width: 20%; }
    @include breakpoint(xs) { width: 100%; }
  }

  &.container-of-1:nth-child(-n + 2):only-child,
  &:only-child { width: 100%; }
}

.footer-link-list {
  float: left;
  text-align: left;

  .social-menu-only & {
    text-align: center;

    @include breakpoint(sm) { text-align: left; }
  }

  .container-of-1 & {
    width: 100%;
  }

  .container-of-2 & {
    width: 50%;
  }

  .container-of-3 & {
    width: 33.33%;
  }

  &,
  .container-of-1 &,
  .container-of-2 &,
  .container-of-3 & {

    @include breakpoint(md) {
      width: 50%;
      padding-bottom: $gutter;

      &:nth-child(2n + 1) {
        clear: both;
      }
    }

    @include breakpoint(sm) {
      width: 100%;
      padding-bottom: $gutter;
    }
  }
}

.footer-link-list-links {
  display: block;
  padding: 10px 0 0;
  margin: 0;
}

.footer-link-list-links-item {
  display: block;
  padding: 5px 0;
  margin: 0;

  .social-menu-only & {
    display: inline-block;
    margin-right: 10px;

    &:last-child { margin-right: 0; }

    @include breakpoint(md) {
      display: block;
      margin-right: 0;
    }
  }
}
.footer-link-list-links-item i{font-size:17px;margin-right:5px;}
.footer-link-list-links-item-anchor:hover{color:#b99800}
.footer-link-list-links-item-anchor {
  display: inline-block;
  vertical-align: top;
  color: $body-text-color;

  &:focus,
  &:hover {
    .style-dark & { color: $white; }
  }

  .style-dark & { color: $white; }
}

.footer-link-list.social-icons {
  a {
    padding-left: 30px;
    position: relative;

    @include breakpoint(sm) {
      padding-left: 20px;
    }
  }
}

// QUOTE

.footer-quote-container {
  float: left;
  width: 30%;

  @include breakpoint(md) {
    width: 100%;
  }

  p:first-child {
    padding-top: 0;
  }
}

// COPYRIGHT

.footer-information {
  display: block;
  float: left;
  width: 100%;
  color: $body-text-color;
  text-align: center;

  .style-dark & {
    &,
    a,
    a:hover,
    a:focus {
      color: $white;
    }
  }

  p {
    margin-top: 0;
    margin-bottom: 0;
  }
}

.payment-icons {
  display: inline-block;
  padding: 0;
  margin: 0 -10px;
  text-align: center;
  vertical-align: middle;

  li {
    @include share-icons($font-size: 30px);
    display: inline-block;
    margin: 10px 10px 0;

    &:last-child { margin-left: 0; }
  }

  .payment-icon-amazon-payments:before { content: $icon-amazon-payments; }
  .payment-icon-american-express:before { content: $icon-american-express; }
  .payment-icon-bitcoin:before { content: $icon-bitcoin; }
  .payment-icon-dankort:before { content: $icon-dankort; }
  .payment-icon-diners-club:before { content: $icon-diners-club; }
  .payment-icon-discover:before { content: $icon-discover; }
  .payment-icon-dogecoin:before,
  .payment-icon-dodgecoin:before { content: $icon-dogecoin; }
  .payment-icon-dwolla:before { content: $icon-dwolla; }
  .payment-icon-forbrugsforeningen:before { content: $icon-forbrugsforeningen; }
  .payment-icon-jcb:before { content: $icon-jcb; }
  .payment-icon-litecoin:before { content: $icon-litecoin; }
  .payment-icon-maestro:before { content: $icon-maestro; }
  .payment-icon-master:before { content: $icon-mastercard; }
  .payment-icon-paypal:before { content: $icon-paypal; }
  .payment-icon-visa:before { content: $icon-visa; }

  // The following are not currently supported by Shopify
  // but are included here as they may be in the future
  .payment-icon-cirrus:before { content: $icon-cirrus; }
  .payment-icon-interac:before { content: $icon-interac; }
  .payment-icon-stripe:before { content: $icon-stripe; }
}

.footer-widget {
  padding: 0;
}

.footer-widget-body {
  width: 100%;
  font-size: 0;
}

.footer-widget-item {
  display: block;

  .column-2 & {
    display: inline-block;
    width: 50%;
    vertical-align: middle;
    font-size: 0;

    @include breakpoint(l) {
      display: block;
      width: 100%;
    }
  }
}

// TWITTER

.twitter-widget {
  font-size: $body-font-size-1;

  @include breakpoint(l) {
    padding: $gutter*3;
  }

  @include breakpoint(sm) {
    padding: $gutter;
  }

  .full-width & {
    padding: $gutter*3 $gutter;

    @include breakpoint(sm) {
      padding: $gutter;
    }
  }

  p {
    padding: 0 0 $gutter/2;
    max-width: 500px;
    margin: auto;

    &:last-child {
      padding: 0;
    }
  }

  a {
    .style-gradient &,
    .style-color & {
      color: $white;

      &:focus,
      &:hover {
        color: $white;
      }
    }
  }

  img {
    display: none;
  }

  .user {
    padding: 0 0 $gutter;
  }

  .user a {
    font-family: $heading-font;
    font-size: $heading-font-size-5;
    line-height: $heading-font-size-5 + 10;
    font-weight: 700;
    color: inherit;
  }

  .user span[data-scribe='element:name'] {
    display: none;
  }

  .user img {
    display: none;
  }

  .timePosted {
    text-transform: uppercase;
    font-weight: 700;
    opacity: 0.8;
  }

  .icon-twitter {
    font-size: 24px;
    line-height: 34px;
  }
}

.twitter-follow {
  display: none;
}

// INSTAGRAM

.instagram .module {padding: $gutter;}

.instagram-widget {
  font-size: 0;
  width: 100%;
  padding: 0;
  margin: 0;
}

.instagram-photo {
  display: inline-block;
  width: 33.33%;

  @include breakpoint(l) {
    width: 20%;
  }

  @include breakpoint(sm) {
    width: 50%;
  }

  &:nth-child(5),
  &:nth-child(6) {
    @include breakpoint(sm) { display: none; }
  }

  &:nth-child(6) {
    @include breakpoint(l) {
      display: none;
    }
  }

  .full-width & {
    width: 20%;

    @include breakpoint(sm) { width: 50%; }

    &:nth-child(6) { display: none; }
  }

  img {
    display: block;
    width: 100%;
  }
}

.module-header-goal-wrapper {
  .has-sticky-header & {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 99;

    @include breakpoint(sm) {
      position: relative;
    }
  }
}

.module-header-goal {
  @include transition(all .1s $ease);
  position: relative;
  height: 0;
  padding: 0;
  text-align: center;
  overflow: hidden;
  opacity: 0;

  &.active {
    height: auto;
    padding: $gutter/2 $gutter;
    opacity: 1;
  }

  .showing-drawer & {
    height: 0;
    padding: 0;
    opacity: 0;
  }

  .template-index & {
    display: none;
  }

  .module-header-body {
    .style-color &,
    .style-gradient &,
    .style-image & {
      color: $white;
    }
  }
}

.module-header-goal-item {
  display: inline-block;
  vertical-align: top;
  line-height: 24px;
  font-size: 14px;
}

.module-header-goal-countdown,
.module-header-goal-time-up {
  @include breakpoint(sm){
    display: none;
  }
}

.module-header-goal-item-icon {
  padding: 0 10px;
  font-size: 18px;

  @include breakpoint(sm) {
    &.icon-arrow-right {
      display: none;
    }
  }
}

.branding {
  float: left;
  max-width: 50%;

  @include breakpoint(sm) {
    float: none;
    max-width: initial;
  }

  @include breakpoint(540px) {
    padding-right: 80px;
    padding-left: 80px;
  }
}

.branding-logo-link {
  position: relative;
  display: block;
  height: $header-height;
  max-width: $logo-max-width;
  padding-top: 10px;
  padding-bottom: 10px;
  line-height: $header-height - 20px;
  color: $primary-color;

  @include breakpoint(sm) {
    height: $header-small-height;
    max-width: 100%;
    margin-right: auto;
    margin-left: auto;

    @if($logo-use-image){
      line-height: $header-small-height - 20px;
    } @else {
      line-height: $header-small-height;
    }
  }

  .showing-drawer & {
    color: $white;
  }

  @media (min-width: 721px) {
    .has-slideshow-full-window.slide-color-dark:not(.drawer-visible) .header:not(.scrolled) & { color: $black; }
    .has-slideshow-full-window.slide-color-light:not(.drawer-visible) .header:not(.scrolled) & { color: $white; }
  }
}

.branding-logo-heading {
  margin: 0;
  font-family: $heading-font;
  font-size: 28px;
  font-weight: $heading-weight;
  line-height: $header-height - 20px;
  color: inherit;

  @include breakpoint(sm) {
    display: block;
    height: $header-small-height;
    max-width: 100%;
    padding-right: 10px;
    padding-left: 10px;
    margin: -10px auto;
    overflow: hidden;
    line-height: $header-small-height;
    text-overflow: ellipsis;
    white-space: nowrap;
  }

  @include breakpoint(xs) {
    font-size: 20px;
  }
}

.branding-logo-image {
  width: initial;
  max-width: $logo-max-width;
  max-height: $logo-max-height;
  margin: -2px auto 0;
  vertical-align: middle;
  transition: opacity 0.2s ease;

  @include breakpoint(sm) {
    max-width: 100%;
    max-height: 60px;
  }
}

.logo-regular {
  display: inline-block;

  .has-slideshow-full-window.slide-color-light .header:not(.scrolled) .alternate-logo-in-drawer & {
    display: none;

    @include breakpoint(sm) {
      display: inline-block;
    }
  }

  .slide-color-light.showing-drawer .header:not(.scrolled) .alternate-logo-in-drawer &,
  .showing-drawer .alternate-logo-in-drawer & {
     display: none;
  }

  // Regardless of all other possible options, of there is only one logo
  // then the main logo _must_ show
  .regular-logo-in-drawer & {
    display: inline-block;
  }
}

.logo-alternate {
  display: none;

  .has-slideshow-full-window.slide-color-light .header:not(.scrolled) & {
    display: inline-block;

    @include breakpoint(sm) {
      display: none;
    }
  }

  .showing-drawer .regular-logo-in-drawer & {
    display: none;
  }

  .slide-color-light.showing-drawer .header:not(.scrolled) &,
  .showing-drawer .alternate-logo-in-drawer & {
    display: inline-block;
  }
}

// STICKY HEADER

body {
  &.has-sticky-header {
    padding-top: $header-height;

    @include breakpoint(sm) { padding-top: 0; }

    &.has-slideshow-full-window { padding-top: 0 !important; }

    &.has-goal-enabled {
      padding-top: $header-height + $header-goal-height;

      @include breakpoint(sm) { padding-top: 0; }

      .no-js & {
        padding-top: $header-height;

        @include breakpoint(sm) { padding-top: 0; }
      }

      &.has-goal-expired { padding-top: $header-height; }
    }
  }
}

// HEADER

.header {
  @include transition(background 0.2s $ease);
  position: relative;
  display: block;
  width: 100%;
  border-bottom: 1px solid #dbdbdb;

  &.full-window-slideshow {
    top: 0;
    right: 0;
    left: 0;
    position: absolute;
    box-shadow: none;
    background: none;

    @include breakpoint(sm) { position: relative; }

    .showing-drawer & { position: relative; }
  }

  .showing-drawer & {
    height: 100%;
    overflow-y: auto;
    background-color: rgba($dark-color, 0.98) !important;
  }

  &.sticky-header {
    @include transition (all 0.2s $ease);
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    z-index: 99;

    @include breakpoint(sm-up){
      &.scrolled {
        background-color: rgba($white, 0.9);
      }

      &.header-bleed-light-colors {
        background-color: rgba($black, 0.4);
      }

      &.lower-than-slideshow {
        background-color: rgba($white, 0.9);

        &.header-bleed-light-colors {
          background-color: rgba($black, 0.4);
        }
      }
    }

    .showing-drawer & { position: fixed; }

    @include breakpoint(sm) {
      position: relative;
      left: auto;
      top: auto;
    }

    .has-goal-enabled & {
      top: $header-goal-height;

      @include breakpoint(sm) { top: auto; }
    }

    .has-goal-expired & { top: 0; }

    .no-js .has-goal-enabled &,
    .showing-drawer &,
    .template-index & {
      top: 0;
    }
  }
}

.main-header-wrapper {
  position: relative;
  max-width: $max-width + ($gutter * 2);
  padding-right: $gutter;
  padding-left: $gutter;
  margin: 0 auto;
  z-index: 110;

  @include breakpoint(l) {
    padding-right: 0;
    padding-left: 0;
  }
}

.main-header {
  position: relative;
  padding: 0;
  height: $header-height;
  z-index: 3;
  border-bottom: 1px solid $border-color;

  &:after,
  &:before {
    @include transition(border-bottom-color .2s $ease);
    position: absolute;
    top: 0;
    left: 100%;
    height: calc(100% + 1px);
    width: 0;
    content: "";
    border-bottom: 1px solid $border-color;

    .showing-drawer & {
      border-bottom-color: rgba($white, 0.25);
    }
  }

  &:after {
    left: auto;
    right: 100%;
  }

  .scrolled & {
    &:after,
    &:before {
      width: 50%;
    }
  }

  .no-border & {
    border: 0;
  }

  .higher-than-slideshow & {
    background: none;
    border-color: rgba($border-color, 0.25);
  }

  .showing-drawer & {
    border-color: rgba($white, 0.25);
  }

  @include breakpoint(l) {
    padding: 0 $gutter;
  }

  @include breakpoint(sm) {
    height: $header-small-height;
    padding: 0;
    text-align: center;
    overflow: hidden;
  }

  @extend .clearfix;
}

// NAVIGATION

.header-tools {
  height: $header-height;
  float: right;

  .header-search-wrapper.active + & {
    opacity: 0;

    @include lt-ie9 {
      visibility: hidden;
    }
  }

  @include breakpoint(sm) {
    float: none;
  }
}

@import "navigation";

.header-actions {
  position: relative;
  line-height: $header-height;
  height: $header-height;
  display: inline-block;
  vertical-align: top;
  text-align: left;

  &:before {
    @include transition(background-color .2s $ease);
    display: inline-block;
    width: 1px;
    height: 16px;
    margin: 0 10px;
    vertical-align: middle;
    background-color: $border-color;
    content: "";

    .showing-drawer & {
      background-color: rgba($white, 0.25);
    }

    .higher-than-slideshow & {
      background-color: rgba($border-color, 0.25);
    }

    .lower-than-slideshow & {
      background-color: $body-text-color;
    }
  }

  @include breakpoint(sm) {
    position: static;
    display: block;
    height: auto;
    text-align: center;
  }
}

.header-actions-list {
  display: inline-block;
  vertical-align: middle;
}

.header-actions-list-item {
  @include transition(color 0.2s $ease);
  @include launch-icons(21px);
  position: relative;
  float: left;
  padding: 0;
  margin-left: $gutter;
  line-height: 21px;
  color: $body-text-color;
  cursor: pointer;

  @media (min-width: 721px) {
    .has-slideshow-full-window.slide-color-dark:not(.drawer-visible) .header:not(.scrolled) & { color: $black; }
    .has-slideshow-full-window.slide-color-light:not(.drawer-visible) .header:not(.scrolled) & { color: $white; }
  }

  .slide-color-light .scrolled &,
  .slide-color-dark .scrolled & {
    color: $body-text-color;
  }

  @include breakpoint(sm) {
    padding: 0;
  }

  &:first-child {
    padding: 0 0 0 $gutter/2;
  }

  &:hover {
    color: $primary-color;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        color: $white;
      }

      .header-bleed-dark-colors & {
        color: $black;
      }
    }
  }
}

.collapsed-navigation {
  .header-actions:before {
    display: none;
  }
}

// CURRENCY SWITCHER

.header-currency-switcher {
  margin-left: 20px;
  opacity: 1;

  .slide-color-light &,
  .slide-color-dark & {
    opacity: 1;
  }

  @include breakpoint(sm) {
    display: none;
  }

  .select-wrapper-dropdown {
    max-height: none;
  }

  .showing-drawer & {
    display: none;
  }
}

// HEADER

.header-currency-switcher {
  .select-wrapper {
    position: relative;
    display: block;
    height: 21px;
    line-height: 21px;
    padding-right: 25px;
    margin: 0;
    background: none;
    font-size: $header-font-size;
    border: 0;

    &:after {
      right: 0;
      line-height: 21px;
      color: $body-text-color;

      @media (min-width: 721px) {
        .has-slideshow-full-window.slide-color-dark .header:not(.scrolled) & { color: $black; }
        .has-slideshow-full-window.slide-color-light .header:not(.scrolled) & { color: $white; }
      }
    }

    &:hover:after,
    &:hover {
      color: $primary-color;
    }

    &.active {
      overflow: hidden;
      outline: $outline-setting;
    }

    @media (min-width: 721px) {
      .has-slideshow-full-window.slide-color-dark .header:not(.scrolled) & { color: $black; }
      .has-slideshow-full-window.slide-color-light .header:not(.scrolled) & { color: $white; }
    }
  }

  .selected-currency {
    @include font-smoothing();
    position: relative;
    height: 100%;
    padding: 0;
    line-height: inherit;
    font-size: $header-font-size;
    font-weight: normal;
    letter-spacing: 2px;
    font-family: $body-font;
  }

  .select-wrapper-dropdown {
    @include border-radius(5px);
    top: -13px;
    margin-left: -10px;
    padding: 45px 0 0;
    z-index: 1;
  }
}

// DRAWER

.drawer-currency-switcher {
  display: block;
  margin: 0 0 $gutter 0;
}

.drawer-currency-switcher .select-wrapper {
  display: inline-block;
  width: 220px;
  height: 44px;
  padding: 0;
  margin: 0;
  vertical-align: top;
  line-height: 44px;
  font-size: $drawer-font-size;
  background: none;
  color: $white;

  &:after {
    line-height: 44px;
    color: $white;
  }

  &.active {
    @include border-radius(5px);
    height: auto;
    background: $white;
    border-color: $border-color;
    color: $body-text-color;

    &:after {
      color: $body-text-color;
    }
  }
}

.drawer-currency-switcher .selected-currency {
  @include ellipsis();
  height: 44px;
  line-height: 44px;
  padding: 0 40px 0 10px;
  color: $white;
  font-size: $heading-font-size-4;
}

.drawer-currency-switcher .active .selected-currency {
  height: auto;
  line-height: inherit;
  color: $body-text-color;
}

.drawer-currency-switcher .select-wrapper-dropdown {
  @include border-radius(0);
  margin: 0;
  background: transparent;
}

.drawer-currency-switcher .select-wrapper-option {
  text-align: left;
  text-indent: 10px;
  font-size: $heading-font-size-4;

  &:first-child {
    border-top: 1px solid $border-color;
  }
}

// SEARCH

.header-search-toggle {
  .showing-drawer & {
    color: $white;

    &:hover {
      color: $white;
    }
  }

  @include breakpoint(sm) {
    display: none;
  }
}

.header-search-wrapper {
  position: absolute;
  top: 0;
  right: 0;
  width: 400px;
  height: 100%;
  overflow: hidden;

  @include breakpoint(sm) {
    display: none;
  }
}

.header-search-form {
  @include transition(all 0.2s $ease);
  width: 300px;
  position: absolute;
  bottom: 100%;
  right: 0;
  z-index: 99;
  background: $border-color;
  color: $body-text-color;
  padding: 0 $gutter/2;
  line-height: $heading-font-size-2;

  @include breakpoint(l) {
    right: $gutter;
  }

  @media (min-width: 721px) {
    .header-bleed-light-colors & {
      color: $white;
      background-color: rgba($white, 0.1);
    }

    .header-bleed-dark-colors & {
      color: $black;
      background-color: rgba($white, 0.1);
    }
  }

  .active & {
    bottom: 40px;
  }

  &:before {
    @include launch-icons(24px);
    position: absolute;
    left: 10px;
    top: 2px;
    content: '\E036';
    color: $body-text-color;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        color: $white;
      }

      .header-bleed-dark-colors & {
        color: $black;
      }
    }
  }
}

input.header-search-input {
  @include transition(all 0.2s $ease);
  display: inline-block;
  vertical-align: top;
  width: 100%;
  height: 42px;
  padding: 0;
  background: none;
  border: 0;
  border-radius: 0;
  text-indent: $gutter;
  color: $body-text-color;

  @include lt-ie9 {
    line-height: 42px;
  }

  @media (min-width: 721px) {
    .header-bleed-light-colors & {
      color: $white;
    }

    .header-bleed-dark-colors & {
      color: $black;
    }
  }

  @include placeholder {
    color: $body-text-color;
    opacity: 0.7;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        color: $white;
        opacity: 0.7;
      }

      .header-bleed-dark-colors & {
        color: $black;
        opacity: 0.7;
      }
    }
  }
}

// ACCOUNT

.header-account-link {

  .collapsed-navigation & {
    display: none;
  }

  @include breakpoint(sm) {
    display: none;
  }
}

// CART

.header-cart-link,
.drawer-toggle {
  @include breakpoint(sm) {
    position: absolute;
    top: 10px;
    display: block;
    width: 60px;
    height: 60px;
    margin: 0;
    line-height: 60px;
  }
}

.header-cart-link {
  font-size: 22px;

  &:hover {
    .header-cart-count {
      color: $white;
    }
  }

  @include breakpoint(sm) {
    left: 10px;
  }

  .showing-drawer & {
    color: $white;
    &:hover {
      color: $white;
    }
  }

  .header-cart-count {
    @include border-radius(100%);
    position: absolute;
    top: -2px;
    right: -5px;
    display: inline-block;
    height: 12px;
    width: 12px;
    margin: 0;
    padding: 0;
    font-family: "Open Sans", sans-serif;
    font-size: 0;
    background-color: $secondary-color;
    border: 2px solid $white;
    z-index: 10;
    display: none;

    @include breakpoint(sm) {
      right: 14px;
      top: 16px;
    }

    &.active {
      display: block;
    }
  }
}

// DRAWER

.header-drawer {
  display: block;
  width: 100%;
  height: 0;
  max-width: $max-width;
  margin: 0 auto;
  text-align: center;
  overflow: hidden;

  .showing-drawer & {
    height: auto;
  }
}

.drawer-toggle {
  position: relative;

  @include breakpoint(sm-up){
    display: none;
  }


  .showing-drawer & span {
    color: $white;
  }

  &:hover {
    color: $primary-color;
  }

  &:before {
    float: left;
    height: 16px;
    width: 1px;
    margin: 3px 25px 0 0px;
    background-color: $border-color;
    content: "";

    .header-bleed-light-colors & {
      background-color: rgba($white, 0.25);
    }

    .header-bleed-dark-colors & {
      background-color: rgba($black, 0.25);
    }
  }

  .showing-drawer & {
    display: inline-block;

    &:before {
      background-color: rgba($white, 0.1);
    }
  }

  .collapsed-navigation & {
    display: inline-block;
  }

  @include breakpoint(sm) {
    position: absolute;
    right: 10px;

    &:before {
      display: none;
    }
  }

  .icon-cross {
    display: none;

    .showing-drawer & {
      display: inline-block;
    }
  }

  .icon-menu {
    display: inline-block;

    .showing-drawer & {
      display: none;
    }
  }
}


.drawer-account-link {
  margin-bottom: $gutter;
  background: $white;
  border-color: $white;
  color: $primary-color;

  &:hover {
    background: transparent;
    border-color: $white;
    color: $white;
  }
}

.drawer-social-icons {
  text-align: center;
  margin-bottom: 75px;

  @include breakpoint(sm) {
    margin-bottom: $gutter*2;
  }

  a {
    position: relative;
    padding: 0 5px;
    font-size: 0;
    display: inline-block;
    vertical-align: top;
  }

  a:before {
    @include font-smoothing();
    font-family: 'social-icons';
    position: relative;
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    font-size: 25px;
    color: $white;
  }

  a[href*="facebook"]:before {
    content: "\e041";
  }

  a[href*="twitter"]:before {
    content: "\e040";
  }

  a[href*="google"]:before {
    content: "\e042";
  }

  a[href*="pinterest"]:before {
    content: "\e043";
  }

  a[href*="instagram"]:before {
    content: "\e057";
  }

  a[href*="vimeo"]:before {
    content: "\e052";
  }

  a[href*="youtube"]:before {
    content: "\e051";
  }
}


