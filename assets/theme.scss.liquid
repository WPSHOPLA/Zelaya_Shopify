.video-wrapper {
  @include background-size(cover);
  position: relative;
  display: block;
  background-position: center;
  background-repeat: no-repeat;

  &:hover {
    .video-wrapper-button {
      background-color: $primary-color;
    }
  }
}

.video-wrapper-overlay {
  @include transition(0.2s linear opacity);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
  background-color: rgba($black,0.7);

  &.inactive {
    opacity: 0;
  }
}

.video-wrapper-text-container {
  @include transform(translateY(-50%));
  position: relative;
  top: 50%;
  display: inline-block;
  vertical-align: middle;
  max-width: 700px;
  margin: auto;
  padding: 0;
  color: $white;
}

.video-wrapper-button {
  @include transition(all 0.2s $ease);
  @include border-radius(100%);
  display: inline-block;
  width: 100px;
  height: 100px;
  margin: $gutter 0 0;
  line-height: 100px;
  text-align: center;
  background-color: $primary-color;

  @include breakpoint(sm) {
    width: 60px;
    height: 60px;
    margin-top: $gutter / 2;
    line-height: 60px;
  }

  &:first-child { margin-top: 0; }
}

.video-wrapper-triangle {
  display: inline-block;
  vertical-align: middle;
  width: 0;
  height: 0;
  margin: -2px -6px 0 0;
  border-top: 20px solid transparent;
  border-bottom: 20px solid transparent;
  border-left: 30px solid $white;

  @include breakpoint(sm) {
    margin: -1px -3px 0 0;
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    border-left: 15px solid $white;
  }
}

.video-wrapper-title {
  display: block;
  margin: 0;
  padding: 0 $gutter/2;
  font-family: $heading-font;
  font-weight: $heading-weight;
  font-size: $heading-font-size-2;
  line-height: $heading-font-size-2 + 10;
  color: $white;

  @include breakpoint(sm) {
    @include ellipsis();
    height: $body-font-size-1 + 10;
    padding: 0;
    line-height: $body-font-size-1 + 10;
    font-size: $body-font-size-1;
    overflow: hidden;
  }
}

.video-wrapper-subtitle {
  display: block;
  max-width: 700px;
  margin: $gutter/2 auto 0;
  padding: 0 $gutter/2;
  font-family: $body-font;
  font-weight: $body-weight;
  color: $white;
  font-size: $subtitle-font-size;
  line-height: $subtitle-font-size + 10;
  opacity: 0.8;

  @include breakpoint(md) {
    height: ($subtitle-font-size + 10) * 2;
    overflow: hidden;
  }

  @include breakpoint(sm) {
    display: none;
  }
}

iframe {
  .video-wrapper &,
  .full-width-video & {
    opacity: 0;
  }

  .fluid-width-video-wrapper & {
    background-color: $dark-color;
    opacity: 1;

    .video-wrapper & {
      opacity: 0;
    }

    .active & {
      opacity: 1;
    }
  }
}

.full-width-video {
  position: relative;
  z-index: 1;

  &.active {
    z-index: 1000;
  }
}

.fluid-width-video-wrapper {
  position: relative;
  z-index: 1;
  width: 100%;
  padding: 0;
  cursor: pointer;

  iframe, object, embed {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

.template-404 {
  .main-content {
    text-align: center;
  }
}
.about {
