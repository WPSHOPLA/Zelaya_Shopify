/* General
-------------------------------- */

body {
  font-family: $body-font;
  color: $body-text-color;
  font-weight: $body-weight;
  font-size: $body-font-size-1;
  line-height: 1.625;

  @if($use-font-smoothing) {
    @include font-smoothing();
  }
}

.icon {
  @include launch-icons();
}

/* Links */

a {
  color: $primary-color;
  text-decoration: none;
  cursor: pointer;
  @include transition(all 0.2s $ease);

  &:focus,
  &:hover {
    color: $primary-color;
  }

  &.disabled {
    color: $body-text-color;
    cursor: default;

    &:focus,
    &:hover {
      color: $body-text-color;
      cursor: default;
    }
  }
}

/* Headers */

h1, h2, h3, h4, h5, h6 {
  font-family: $heading-font;
  font-weight: $heading-weight;
  font-size: inherit;
  margin: 0;
  padding: 0;
  -webkit-font-smoothing: initial;

  @if($use-font-smoothing) {
    @include font-smoothing();
  }
}

/* Basic text */

img {
  max-width: 100%;
  display: inline-block;
  vertical-align: top;
}

ul, ol, dl {
  padding: 0;
  margin: 23px 0;
}

ul {
  list-style: outside none;
}

ol {
  list-style: outside decimal;
}

li {
  margin: 12px 0;
}

hr {
  height: 2px;
  background: $border-color;
  border: 0;
}

p {
  margin: 0;
  padding: $gutter/2 0;
}

strong {
  font-weight: 700;

  @if($use-font-smoothing) {
    @include font-smoothing();
  }
}

.bold {
  font-weight: 700;
}

/* Tables */

table {
  color: $body-text-color;
  border: 1px solid $border-color;
  border-top: none;
  font-size: 14px;
  border-collapse: separate;
  margin: $gutter 0;
  background: transparent;

  &.clean {
    border: 0;
    border-radius: 0;
    font-size: $body-font-size-1;
  }

  .column-title {
    display: none;
  }
}

th {
  background: $border-color;
  color: $heading-text-color;
  font-weight: $heading-weight;
  text-align: center;

  .clean & {
    background: none;
  }
}

tr td, tr th {
  border-top: 1px solid $border-color;
  padding: $gutter/(3/2);
}

tr td {
  border-left: 1px solid $border-color;

  &:first-child {
    border-left: none;
  }
}

table.mobile-layout {
  border-bottom: none;

  .column-title {
    font-weight: 700;
    width: 114px;
    padding-right: 23px;
    display: inline-block;
  }

  thead {
    display: none;
  }

  tr td {
    width: 100%;
    display: block;
    text-align: left;
    border-top: 1px solid $border-color !important;
    border-left: none;

    /* < IE 10 fix */
    float: left;
    clear: left;
  }

  tr:nth-child(even) td {
    background: $border-color;
  }
}

/* Buttons */

.button,
.submit,
button {
  @include border-radius($button-height/2);
  @include box-sizing(border-box);
  @include transition(all 0.2s $ease);
  @include font-smoothing(none);
  position: relative;
  display: inline-block;
  vertical-align: top;
  padding: $gutter/2 $gutter;
  font-family: $button-font;
  font-weight: $button-weight;
  color: $primary-color;
  font-size: 15px;
  line-height: 22px;
  letter-spacing: 2px;
  text-align: center;
  text-transform: uppercase;
  background-color: transparent;
  border: 1px solid $primary-color;
  cursor: pointer;
  font-weight: bolder;

  @include breakpoint(sm) {
    text-align: center;
    font-size: 12px;
    line-height: 18px;
  }

  .product-form-submit-wrap & {
    background-color: $white;
  }

  &:focus,
  &:hover {
    background: $primary-color;
    border-color: $primary-color;
    color: $white;
  }

  &.secondary {
    background: transparent;
    border-color: $secondary-color;
    color: $secondary-color;

    &:focus,
    &:hover {
      background: $secondary-color;
      border-color: $secondary-color;
      color: $white;
    }

    &.inverse {
      background: $secondary-color;
      border-color: $secondary-color;
      color: $white;

      &:focus,
      &:hover {
        background: transparent;
        border-color: $secondary-color;
        color: $secondary-color;
      }
    }
  }

  &.disabled {
    background: $border-color;
    color: $body-text-color;
    border-color: $border-color;
    opacity: 1;
    cursor: default;

    &:focus,
    &:hover {
      background: $border-color;
      color: $body-text-color;
      border-color: $border-color;
      opacity: 1;
      cursor: default;
    }
  }

  .style-color &,
  .style-gradient &,
  .overlay-none &,
  .overlay-black &,
  .overlay-color &,
  .overlay-gradient &,
  .overlay-primary & {
    background: transparent;
    border-color:#b99800;
    color: #b99800;

    &:focus,
    &:hover {
      background: #b99800;
      border-color: #fff;
      color: $white;
    }
  }

  .overlay-secondary & {
    background: $white;
    border-color: $white;
    color: $secondary-color;

    &:focus,
    &:hover {
      background: $secondary-color;
      border-color: $white;
      color: $white;
    }
  }

  .icon {
    @include launch-icons();
    display: inline-block;
    vertical-align: top;
    font-size: 18px;
    color: inherit;
    margin-left:8px;
  }

  .icon-arrow-right {
    font-size: 13px;
  }
}

/* Forms & buttons */

form {
  font-size: 14px;
}

label {

  &:hover { cursor: pointer; }

  .inline-input-wrapper & {
    font-weight: normal;
    font-size: 14px;
    margin-left: 5px;
    display: inline-block;
    vertical-align: middle;
  }
}

.input-wrapper {
  margin: $gutter 0;

  &:last-child {
    margin-bottom: 0;
  }
}

.inline-input-wrapper {
  margin: $gutter / 3 0;

  input + .inline-label {
    margin-left: $gutter / 4;
  }
}

input, textarea {
  border-radius: 0;

  label ~ & {
    margin-top: $gutter/3;
  }

  &:focus {
    outline: none;
  }
}
.swachtitle{font-family:"Open Sans",sans-serif;font-size:18px;color:#575757;}
/* 
   Swatches Styles
*/

{% assign width = '30px' %}
{% assign height = '30px' %}
.swatch { 
  margin:1em 1em 0 1em; 
}
/* Label */
.swatch .header {
  margin: 0.5em 0;
  display:none;
}
/* Hide radio buttons.*/
.swatch input { 
  display:none;
}
.swatch label {
  /* Rounded corners */
  -webkit-border-radius:2px;
  -moz-border-radius:2px;
  border-radius:27px;
  /* To give width and height */
  float:left;
  /* Color swatches contain no text so they need to have a width. */
  min-width:{{ width }} !important; 
  height:{{ height }} !important;
  /* No extra spacing between them */
  margin:0;
  /* The border when the button is not selected */
  border:#ccc 1px solid;
  /* Background color */
  background-color:#ddd;
  /* Styling text */
  font-size:13px;
  text-align:center;
  line-height:{{ height }};
  white-space:nowrap;
  text-transform:uppercase;
}
.swatch-element label { padding:0 10px; }
.color.swatch-element label { padding:0; }
/* Styling selected swatch */
/* Slightly raised */
.swatch input:checked + label{  
    height: 37px;
    border-radius: 100%;
    padding: 2px 2px;
    width: 29px;
  border:1px solid #000;
} 
.swatch .swatch-element {
  float:left;
  -webkit-transform:translateZ(0); /* webkit flicker fix */
  -webkit-font-smoothing:antialiased; /* webkit text rendering fix */
  font-size:20px;
  /* Spacing between buttons */
  margin:0px 10px 10px 0;
  /* To position the sold out graphic and tooltip */
  position:relative;
}
/* Image with the cross in it */
.crossed-out { position:absolute; width:100%; height:100%; left:0; top:0; }
.swatch .swatch-element .crossed-out { display:none; }
.swatch .swatch-element.soldout .crossed-out { display:block; }
.swatch .swatch-element.soldout label {
  filter: alpha(opacity=60); /* internet explorer */
  -khtml-opacity: 0.6;      /* khtml, old safari */
  -moz-opacity: 0.6;       /* mozilla, netscape */
  opacity: 0.6;           /* fx, safari, opera */
}
/* Tooltips */
.swatch .tooltip {
  text-align:center;
  background:gray;
  color:#fff;
  bottom:100%;
  padding: 10px;
  display:block;
  position:absolute;
  width:100px;
  left:{{ width | remove: 'px' | to_number | divided_by: 2 | minus: 50 | plus: 2 }}px;
  margin-bottom:15px;
  /* Make it invisible by default */
  filter:alpha(opacity=0);
  -khtml-opacity: 0;
  -moz-opacity: 0;
  opacity:0;
  visibility:hidden;
  /* Animations */
  -webkit-transform: translateY(10px);
     -moz-transform: translateY(10px);
      -ms-transform: translateY(10px);
       -o-transform: translateY(10px);
          transform: translateY(10px);
  -webkit-transition: all .25s ease-out;
     -moz-transition: all .25s ease-out;
      -ms-transition: all .25s ease-out;
       -o-transition: all .25s ease-out;
          transition: all .25s ease-out;
  -webkit-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
     -moz-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
      -ms-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
       -o-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
          box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
  z-index: 10000;
  -moz-box-sizing:border-box; 
  -webkit-box-sizing:border-box; 
  box-sizing:border-box;
}
.swatch .tooltip:before {
  bottom:-20px;
  content:" ";
  display:block;
  height:20px;
  left:0;
  position:absolute;
  width:100%;
}
/* CSS triangle */
.swatch .tooltip:after {
  border-left:solid transparent 10px;
  border-right:solid transparent 10px;
  border-top:solid gray 10px;
  bottom:-10px;
  content:" ";
  height:0;
  left:50%;
  margin-left:-13px;
  position:absolute;
  width:0;
}
.swatch .swatch-element:hover .tooltip {
  filter:alpha(opacity=100);
  -khtml-opacity:1;
  -moz-opacity:1;
  opacity:1;
  visibility:visible;
  -webkit-transform:translateY(0px);
     -moz-transform:translateY(0px);
      -ms-transform:translateY(0px);
       -o-transform:translateY(0px);
          transform:translateY(0px);
}
.swatch.error {
  background-color:#E8D2D2!important;
  color:#333!important;
  padding:1em;
  border-radius:5px;
}
.swatch.error p {
  margin:0.7em 0;
}
.swatch.error p:first-child {
  margin-top:0;
}
.swatch.error p:last-child {
  margin-bottom:0;
}
.swatch.error code {
  font-family:monospace;
}
