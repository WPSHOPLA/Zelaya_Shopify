input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="telephone"],
input[type="tel"],
input[type="number"],
textarea {
  display: block;
  width: 100%;
  border: 1px solid $border-color;
  color: #000000;
  font-family: $body-font;
  border-radius: 5px;
  -webkit-appearance: none;
 	
  @include placeholder {
    color: #000;
    opacity: 0.7;
  }

  &.error {
    color: $error-color;
    @include placeholder {
      color: $error-color;
      opacity: 0.7;
    }
  }
}

.error-message .errors {
  color: $error-color;
}

input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="telephone"],
input[type="tel"],
input[type="number"] {
  height: 50px;
  line-height: 50px;
  padding: 0 15px;

  @include lt-ie9 {
    padding: 0 15px;
    line-height: 50px;
  }
}

input[type="password"] {
  @include lt-ie9 {
    font-family: Arial, sans-serif;
  }
}

// Checkboxes
input[type="checkbox"] {
  &:focus {
    outline: $outline-setting;
  }
}

// INPUT NUMBER CUSTOM STYLING

input[type="number"] {
  @include border-radius(0);
  margin: 0;
  padding: 0;
  border: 0;
  color: $body-text-color;
  -moz-appearance: textfield;

  &::-webkit-outer-spin-button,
  &::-webkit-inner-spin-button {
    -webkit-appearance: none;

    .no-js & {
      -webkit-appearance: number;
      -moz-appearance: number;
    }
  }

  .no-js & {
    height: auto;
    line-height: 1;
    padding: 5px 0;
    color: $black;
    -webkit-appearance: number;
    -moz-appearance: number;
  }
}

.number-input-wrapper {
  @include border-radius(5px);
  position: relative;
  display: inline-block;
  border: 1px solid $border-color;
  overflow: hidden;

  .no-js & {
    margin: $gutter/2 0 0;
  }
}

.number-input-field {
  float: left;
  width: $input-number-width - $input-number-nav-width;
  height: $input-number-height;
  background-color: $white;
  line-height: $input-number-height;

  @include breakpoint(sm) {
    text-align: center;
    width: 100%;
  }

  .cart-item-td & {
    @include breakpoint(md) {
      text-align: center;
      width: 100%;
    }
  }

  input[type="number"] {
    float: left;
    width: 100%;
    height: 100%;
    text-align: center;
    border: 0;
  }

  .no-js & {
    height: auto;
    line-height: 1;
  }
}

.number-input-label { display: none; }

.number-input-nav {
  float: left;
  height: $input-number-height;
  width: $input-number-nav-width;
  border-left: 1px solid $border-color;

  @include breakpoint(sm) {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    border: 0;
  }

  .cart-item-td & {
    @include breakpoint(md) {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      border: 0;
    }
  }

  .no-js & {
    display: none;
  }
}

.number-input-nav-item {
  float: left;
  height: $input-number-height/2;
  line-height: $input-number-height/2;
  width: $input-number-nav-width;
  color: $body-text-color;
  background-color: $white;
  text-align: center;
  cursor: pointer;

  @include breakpoint(sm) {
    position: absolute;
    height: $input-number-height;
    width: $input-number-height;
    line-height: $input-number-height;
  }

  .cart-item-td & {
    @include breakpoint(md) {
      position: absolute;
      height: $input-number-height;
      width: $input-number-height;
      line-height: $input-number-height;
    }
  }

  &:focus,
  &:hover {
    color: $primary-color;
  }

  &:first-child {
    border-bottom: 1px solid $border-color;

    @include breakpoint(sm) {
      right: 0;
      border-left: 1px solid $border-color;
      border-bottom: none;
    }

    .cart-item-td & {
      @include breakpoint(md) {
        right: 0;
        border-left: 1px solid $border-color;
        border-bottom: none;
      }
    }
  }

  &:last-child {
    @include breakpoint(sm) {
      left: 0;
      border-right: 1px solid $border-color;
    }

    .cart-item-td & {
      @include breakpoint(md) {
        left: 0;
        border-right: 1px solid $border-color;
      }
    }
  }
}

// SELECT CUSTOM STYLING

.select-wrapper {
  @include border-radius(5px);
  @include transition(border-color 0.4s $ease);
  position: relative;
  height: $select-height;
  line-height: $select-height;
  border: 1px solid $border-color;
  color: $body-text-color;
  background-color: $white;
  padding: 0 30px 0 0;
  margin: 0;
  font-size: 14px;
  text-align: left;
  cursor: pointer;
  z-index: 55;

  &.active {
    border-color: $primary-color;
  }

  .customer-address-edit-form & { width: 100% !important; }

  .no-js & {
    padding: 0 !important;
    margin-bottom: -2px;
  }

  &:after {
    position: absolute;
    top: 0;
    right: 15px;
    height: 100%;
    line-height: inherit;
    color: $body-text-color;
    content: "\F107";
    @include launch-icons(24px);

    .no-js & {
      display: none;
    }
  }

  select {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 2;
    width: 100%; // needed for Firefox
    height: 100%; // needed for IE
    padding: 0;
    text-transform: none;
    cursor: pointer;
    opacity: 0;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
  }

  .selected-text {
    @include ellipsis();
    padding: 0 15px;
    height: $select-height;
    line-height: $select-height;
    width: 100%;
    z-index: 2;

    .no-js & {
      display: none;
    }
  }
}

// TEXTAREA

textarea {
  padding: $gutter/2;
  max-width: 100%;
}

/* IFRAMES */

iframe {
  border: 0;
  max-width: 100%;
}

.rte {
  ul {
    list-style-type: disc;
  }

  ul li,
  ol li {
    list-style-position: inside;
  }

  h1 {
    margin: 0.67em 0;
    font-size: 2em;
  }

  h2 {
    margin: 0.83em 0;
    font-size: 1.5em;
  }

  h3 {
    margin: 1em 0;
    font-size: 1.17em;
  }

  h4 {
    margin: 1.33em 0;
    font-size: 1em;
  }

  h5 {
    margin: 1.67em 0;
    font-size: 0.83em;
  }

  h6 {
    margin: 2.33em 0;
    font-size: 0.67em;
  }
}

* {
  @include box-sizing(border-box);
}

html {
  height: 100%;
}

body {
  background: $body-background-color;

  &.showing-drawer {
    height: 100%;
    overflow: hidden;
  }
}

.main-content {
  position: relative;
  z-index: 1;
}

//************************************************************************//
// Module
//************************************************************************//

.module {
  max-width: $max-width + $gutter*2;
  margin: 0 auto;
  padding: $gutter*3 $gutter;
  text-align: center;

  @include breakpoint(sm) {
    padding-top: $gutter*2;
    padding-bottom: $gutter*2;
    padding-left: 15px;
    padding-right: 15px;
  }

  &.page {
    padding-top: $gutter*2;

    @include breakpoint(sm) {
      padding-top: $gutter;

      &.no-content {
        padding-bottom: $gutter;
      }
    }
  }

  &.left-right {
    max-width: 100%;
    padding: $gutter*3 0;

    &.collection {
      padding-bottom: 0;
    }
  }

  &.full-width {
    max-width: 100%;
    padding: 0;
  }
}

.module-wrapper {
  position: relative;
  display: block;

  border-bottom: 1px solid #dbdbdb;

}

.module-header {
  position: relative;
  z-index: 1;
  padding-right: $gutter;
  padding-bottom: $gutter * 3;
  padding-left: $gutter;

  @include breakpoint(sm) { 
    padding-bottom: $gutter * 2;
    padding-left:0;
    padding-right: 0;
  }

  &:only-child{ padding-bottom: 0; }
}

.module-body {
  position: relative;
  z-index: 1;

  @include breakpoint(sm) {
    &.has-header {
      padding: $gutter 0 0;
    }
  }
}

.module-title {
  display: block;
  font-family: $heading-font;
  font-weight: $heading-weight;
  font-size: $heading-font-size-2;
  line-height: $heading-font-size-2 + 10;
  color: $heading-text-color;
  margin: 0;
  padding: 0;

  @include breakpoint(sm) {
    font-size: $heading-font-size-2-xs;
    line-height: $heading-font-size-2-xs + 10;
  }

  .style-color &,
  .style-gradient &,
  .style-image & {
    color: $white;
  }

  .light-overlay & {
    color: $heading-text-color;
  }
}

.module-subtitle {
  display: block;
  max-width: 900px;
  margin: 0 auto;
  padding: $gutter/2 0 0;
  font-family: $body-font;
  font-weight: $body-weight;
  color: $body-text-color;
  font-size: $subtitle-font-size;
  line-height: $subtitle-font-size + 10;

  @include breakpoint(sm) {
    font-size: $body-font-size-1;
    line-height: $body-font-size-1 + 10;
  }

  .style-color &,
  .style-gradient &,
  .style-image & {
    color: $white;
  }

  .light-overlay & {
    color: $heading-text-color;
  }
}

.module-content {
  display: block;
  font-size: $body-font-size-1;
  line-height: $body-font-size-1 + 10;
  
  @include breakpoint(sm)
  {
    font-size: $body-font-size-3;
  	line-height: $body-font-size-1 + 8;
  }
  color: $body-text-color;

  .style-color &,
  .style-gradient &,
  .style-image & {
    color: $white;
  }

  .light-overlay & {
    color: $heading-text-color;
  }
}

.about-content .module, .about-content .list-item{
  @include breakpoint(sm)
  {
    padding: 0;
  }
}

.module-cta {
  display: block;
  padding: $gutter 0 0;
}

.module-cta-subline {
  display: block;
  font-size: $body-font-size-1;
  line-height: $body-font-size-1 + 10;
  padding: $gutter/2 0 0;
}

.module-footer {
  position: relative;
  padding: $gutter 0 0;
  z-index: 1;
}

//************************************************************************//
// List: Column list layout
//************************************************************************//

.list {
  position: relative;
  display: block;
  text-align: center;
  font-size: 0;
  padding-right: $gutter / 2;
  padding-left: $gutter / 2;
}

.list-item {
  position: relative;
  display: inline-block;
  vertical-align: top;
  text-align: center;
  padding: 0 $gutter/2;
  margin-bottom: $gutter;

  .column-1 & {
    width: 100%;
    max-width: 600px;
    margin: 0 auto;
  }

  .column-2 & {
    width: 50%;

    @media (max-width: 480px) { width: 100%; }
  }

  .column-3 & {
    width: 33.33%;

    @include breakpoint(sm) { width: 50%; }
    @media (max-width: 480px) { width: 100%; }
  }

  .column-4 & {
    width: 25%;

    @include breakpoint(md) { width: 50%; }
    @include breakpoint(xs) { width: 100%; }
  }

  .inline & {
    @include breakpoint(md) {
      padding: $gutter $gutter/2;
    }
  }

  .random & {
    float: left;

    .list-cover {
      background-position: center;
      background-repeat: no-repeat;
    }

    @include breakpoint(sm) {
      float: none;
      display: block;
    }

    &:nth-child(1n) {
      width: 50%;

      @include breakpoint(sm) {
        width: auto;
      }

      .list-cover {
